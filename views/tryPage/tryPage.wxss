page{
  width: 100%;
  background-color: #F7F7F7;
  padding-top: 90rpx;
}
.content{
  padding:10rpx 20rpx 0;
}
.classBtn{
  font-size: 28rpx;
  color: #777777;
  justify-content: center;
  line-height: 60rpx;
  padding: 30rpx 0;
}
.fixedNav{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index:666; 
  padding-bottom: 30rpx;
  background-color: #ffffff;
}
.timeTop{
  position: fixed;
  width: 100%;
  top: 90rpx;
  left: 0;
  z-index:1000; 
}
.classBtn image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 8rpx;
}
.classBtn > view{
  align-items: center;
}
.classBtn > view:nth-child(2n){
  margin: 0 30rpx;
}
.productList{
  background-color:#F7F7F7; 
  color: #333333;
  font-size: 26rpx;
}
.productList > view > view{
  margin-bottom: 20rpx;
  background-color: #ffffff;
  padding: 30rpx 20rpx 0;
}
.productList > view.typeLast > view{
  margin-bottom: 0;
  border-bottom: 2rpx solid #e1e1e1;
}
.productList > view .title{
  line-height: 60rpx;
}
.productList > view .smallTitle{
  font-size: 24rpx;
  /* line-height: 32rpx; */
  color: #777777;
  padding: 20rpx 0;
}
.productList > view .price{
  font-size: 24rpx;
  color: #777777;
  line-height: 70rpx;
  justify-content: space-between;
}
.productList > view .price text{
  font-size: 34rpx;
  color: #E5006E;
}
.productList > view .price .firstTitle{
  color: #000000;
}
.productList image{
  display: block;
  width: 100%;
}
.timer{
  justify-content: center;
  background-color:#F7F7F7;
  padding: 43rpx 0;
  font-size: 30rpx;
  color: #333333;
  flex-wrap: nowrap;
  overflow-x: auto;
  margin-top: 30rpx;
}
.timer > view{
  margin: 0 27rpx;
  text-align: center;
}
.timer > view view:first-child{
  margin-bottom: 10rpx;
}
.timer > view view:last-child{
  font-size: 22rpx;
  color: #777777
}
.timer > view.activeTime > view{
  color: #E50F72
}
.productList >  view.typeLast > view{
  /* justify-content: space-between; */
  font-size: 28rpx;
  padding: 10rpx 30rpx 20rpx 10rpx;
  color: #333333;
}
.typeLast .rightP{
  padding: 18rpx 0 0 22rpx;
  -webkit-flex: 1;
  -ms-flex: 1;
  -moz-flex: 1;
  -o-flex: 1;
  flex: 1;
  /* text-align: center; */
}
.typeLast > view > view:first-child{
  width: 250rpx;
  height: 250rpx;
}
.typeLast > view image{
  display: block;
  width: 250rpx;
  height: 250rpx;
}
.typeLast > view text{
  line-height: 84rpx;
  font-size: 24rpx;
  color: #e50f72;
}
.typeLast > view .btn{
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
  font-size: 24rpx;
}
.typeLast > view .btn image{
  width: 14rpx;
  height: 24rpx;
  margin-left: 15rpx;
}
.border_radius{
  align-items: center;
  justify-content: center;
}
.typeLast > view .btn view:last-child{
  line-height: 56rpx;
  color: #ffffff;
  text-align: center;
  width: 130rpx;
  background-color: #e50f72;
}
.typeLast > view .btn view:last-child.disno{
  background-color: rgb(252, 137, 178)
}
.typeLast > view > view:first-child{
  position: relative;
}
.typeLast > view > view:first-child .tryOut{
  position: absolute;
  top: 50%;
  left:50%;
  width: 150rpx;
  height: 150rpx;
  margin: -75rpx 0 0 -75rpx;
}
.productList > view .price .newChange{
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.productList > view .price .aloneR{
  color: #777;
  align-items: center;
}
.productList > view .posiImg{
  position: absolute;
  display: block;
  width: 134rpx;
  height: 45rpx;
  left: 50rpx;
  top: 22rpx;
}
.productList .nineImg{
  background-color: #fff;
  padding: 26rpx 30rpx 0;
}
.productList .nineImg .over2{
  font-size: 30rpx;
  color: #333333;
}
.productList .nineImg .over2 image{
  display: inline-block;
  width: 106rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.productList .nineImg .imgaes{
  flex-wrap: wrap;
  /* justify-content: space-between; */
}
.productList .nineImg .imgaes.active{
  justify-content: space-between;
}
.productList .nineImg .imgaes image{
   width: 340rpx;
  height: 340rpx;
  margin-bottom: 12rpx;
}
.productList .nineImg .imgaes .three{
   width: 222rpx;
  height: 222rpx;
  margin-right: 10rpx;
}
.productList .nineImg .imgaes .three:nth-child(3n){
  margin-right: 0;
}
.productList .nineImg .detialP{
  justify-content: space-between;
  background-color: #F5F5F5;
  padding: 10rpx 26rpx 10rpx 10rpx;
  font-size: 26rpx;
  color: #333333;
  align-items: center;
  margin-top: 20rpx;
}
.productList .nineImg .detialP .flex image{
  width: 110rpx;
  height: 110rpx;
  margin-right: 22rpx;
}
.productList .nineImg .detialP .over{
  margin-bottom: 5rpx;
  margin-top: 20rpx;
}
.productList .nineImg .detialP > image{
  width: 42rpx;
  height: 42rpx;
}
.productList .nineImg  .lastd{
  padding: 26rpx 30rpx;
  font-size: 24rpx;
  color: #AAAAAA;
  justify-content: space-between;
  align-items: center;
}
.productList .nineImg  .lastd image{
  display: inline-block;
  vertical-align: middle;
}
.productList .nineImg  .lastd .save{
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
}
.productList .nineImg  .lastd .save2{
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.productList .nineImg  .lastd .BUY{
  width: 126rpx;
  height: 48rpx;
  font-size: 26rpx;
  color: #FFFFFF;
  line-height: 48rpx;
  background-color: #E50F72;
  border-radius: 24rpx;
  text-align: center;
  margin-left: 22rpx;
}
.GoShouQuan{
  position: fixed;
  width:460rpx;
  height: 270rpx;
  top: 50%;
  left: 50%;
  margin: -130rpx 0 0 -260rpx;
  background-color: #ffffff;
  color: #333;
  font-size: 24rpx;
  z-index: 20000;
  padding: 10rpx 30rpx;
  border-radius: 30rpx;
}
.GoShouQuan view:first-child{
  text-align: center;
  font-size:28rpx;
  line-height: 60rpx; 
}
.GoShouQuan view:nth-child(2){
  line-height: 50rpx;
  color: #666666;
  text-align: center;
}
.GoShouQuan .flex{
  margin-top: 30rpx;
  justify-content: space-between;
}
.buyPopver,.couponList{
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    z-index: 1000;
    background: rgba(0,0,0,.5);
 }
 .buyContent,.couponContent{
   position: absolute;
   left: 0;
   width: 100%;
   bottom: 0;
   background-color: #ffffff;
   z-index: 111
 }
 .buyContent image{
   display: block;
   width: 154rpx;
   height: 154rpx;
   margin-top: -20rpx;
   margin-right: 20rpx;
 }
 .buyContent .top{
   border: 2rpx solid #e1e1e1;
   padding: 0 28rpx 30rpx;
 }
 .buyContent .imgbord{
   border:2rpx solid #e1e1e1;
 }
 .buyContent image.close,.couponContent .top .close{
   display: block;
   width: 32rpx;
   height: 32rpx;
   margin: 0;
 }
 .buyContent .right{
   flex: 1;
 }
.buyContent .right .priceBuy{
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #000000;
  line-height: 80rpx;
}
.buyContent .right .priceBuy text{
  font-size: 36rpx;
}
.buyContent .right > text{
  font-size: 20rpx;
  color: #a2a2a2;
  margin-top: 6rpx;
}
.buyContent .center{
  padding: 0 28rpx;
  font-size: 26rpx;
  color: #000000;
  border-bottom: 2rpx solid #cfcfcf;  
  max-height: 700rpx;
  overflow-y: scroll;
}
.buyContent .center .valueItem .index_son{
    border: 2rpx solid #e30067;
    color: #e30067;
}
.buyContent .center > view{
 border-bottom:2rpx solid #e1e1e1;
}
.buyContent .center > view:last-child{
  border: none;
}
.buyContent .center text{
  padding-top: 6rpx;
  line-height: 74rpx;
}
.buyContent .center .valueItem{
  font-size: 24rpx;
  line-height: 56rpx;
}
.buyContent .center .valueItem view{
  text-align: center;  
  min-width: 90rpx;
  border-radius: 2rpx;
  margin:0 20rpx 24rpx 1rpx; 
  border: 2rpx solid #cfcfcf;
  display: inline-block;
  padding: 0 15rpx;
}
.buyContent .buyNumKey{
  padding-bottom: 26rpx;
}
.buyContent .addOrDel{
  align-items: center;
}
.buyContent .addOrDel image{
  display: block;
  height: 56rpx;
  width: 86rpx;
  margin: 0;
}
.buyContent .addOrDel text{
  line-height: 56rpx;
  font-size: 32rpx;
  min-width: 48rpx;
  color: #000000;
  text-align: center;
  margin: 0 20rpx;
}
.buyContent .bottom{
  padding: 18rpx 26rpx;
}
.buyContent .bottom view{
  background-color: #e50f72;
  font-size: 34rpx;
  line-height: 96rpx;
  color: #ffffff;
  text-align: center;
}
