/* views/personal/addCard/addCard.wxss */
page {
  display: block;
  background-color: #f6f6f6;
  
}

.tipsTop{
 text-align: center;
 padding-top: 30rpx;
 font-size: 30rpx;
}
.tipsSure{
  display: flex;
  display:-webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: -moz-flex;
  -webkit-justify-content:space-around;
  justify-content:space-around;
  -moz-box-pack:space-around;
  -webkit--moz-box-pack:space-around;
  border-top: 1rpx solid #e5e5e5;
}
.tipsName{
  width: 50%;
  text-align: center;
  font-size: 30rpx;
  line-height: 100rpx;
}
.shade{
  background: rgba(0, 0, 0, .5);
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 33;
}
.shadeBox{
  width: 75%;
  height: 380rpx;
  background: #fff;
  margin: 0 auto;
 
  border-radius: 10rpx;
  position: relative;
  overflow: hidden;
}
.colseIcon{
  display: block;
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  z-index: 2;
}
.iconTip{
  text-align: center;
  padding-top: 50rpx;
  font-size: 32rpx;
  color: #333;
}
.inputIcon{
  margin-top: 40rpx;
  position: relative;
}
.codeImg{
  display: block;
  width: 140rpx;
  height: 60rpx;
  position: absolute;
  right: 10%;
  top: 50%;
  margin-top: -30rpx;
  z-index: 2;
}
.inputIconIn{
  width: 85%;
  background: #F3F0F3;
  border-radius: 8rpx;
  height: 90rpx;
  margin: 0 auto;
}
.listDetail{
  display: flex;
  display:-webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: -moz-flex;
  -webkit-justify-content:flex-start;
  justify-content:flex-start;
  -moz-box-pack:flex-start;
  -webkit--moz-box-pack:flex-start;
  margin: 20rpx 0;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #333;
  background:#fff;
  padding: 0 20rpx;
  position: relative;
}
.codeDetail{
  position: absolute;
  right: 20rpx;
  top: 50%;
  margin-top: -30rpx;
  font-size: 28rpx;
  color: #e50f72;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  width: 30%;
  border: 1rpx solid #e50f72;
  border-radius: 8rpx;
  z-index: 22
}
.sureCodeGet{
  
  width: 400rpx;
  height: 90rpx;
  background: #F798C4;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  border-radius: 8rpx;
  margin: 0 auto;
  margin-top: 30rpx;
}
.listName{
  width: 25%;
}
.listType{
  display: flex;
  display:-webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: -moz-flex;
  -webkit-justify-content:flex-start;
  justify-content:flex-start;
  -moz-box-pack:flex-start;
  -webkit--moz-box-pack:flex-start;
  -ms-flex-align:center;/* IE 10 */
  -webkit-align-items: center;
  -moz-align-items: center;
  align-items: center;
  height: 80rpx;
}
.listImg{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-left: 20rpx;
}
.inputDetail{
  height: 80rpx;
  font-size: 30rpx;
}
.moreList{
  padding-left: 20rpx;
  background: #fff;
}
.isDefalut{
  margin-top: 20rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #333;
  display: flex;
  display:-webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: -moz-flex;
  -webkit-justify-content:flex-start;
  justify-content:flex-start;
  -moz-box-pack:flex-start;
  -webkit--moz-box-pack:flex-start;
  -ms-flex-align:center;/* IE 10 */
  -webkit-align-items: center;
  -moz-align-items: center;
  align-items: center;
}
.tipBot{
  padding: 0 20rpx;
  color: #e50f72;
  font-size: 30rpx;
  margin-top: 20rpx;
}
.sureBot{
  margin-top: 30rpx;
}
.botSure{
  width: 90%;
  height: 80rpx;
  font-size: 30rpx;
  color: #fff;
  background: #e50f72;
  text-align: center;
  border-radius: 4rpx;
  line-height: 80rpx;
  margin: 0 auto;
}