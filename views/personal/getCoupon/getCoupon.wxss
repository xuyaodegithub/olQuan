/* views/personal/getCoupon/getCoupon.wxss */
page {
  display: block;
  background-color: #f2f2f2;
  padding-bottom: 20rpx;
}
.mainImg{
  display: block;
  width: 100%;
  
}
.couponList{
  padding: 20rpx 20rpx 0;
  position: relative;
}
.coupon{
  width: 100%;
  height: 233rpx;
  position: relative;

}
.bakcImg{
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -2;
}
.couponDescribe{
  display: flex;
  display:-webkit-box;
  display: -moz-box;
  display: -moz-flex;
  display: -ms-flexbox;
  display: -webkit-flex;
  -webkit-justify-content:flex-start;
  justify-content:flex-start;
  -moz-box-pack:flex-start;
  -webkit--moz-box-pack:flex-start;
}
.money{
  width: 200rpx;
  color: #ed0276;
  line-height: 160rpx;
  font-size:60rpx;
  text-align: center;
  font-weight: 600;
}
.describe{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  margin-left: 20rpx;
  padding-top: 28rpx;
  font-size: 24rpx;
}
.couponName{
  color: #333;
  width: 470rpx;
  line-height: 32rpx;
  height: 64rpx;
  overflow: hidden;
}
.getCoupon{
  margin-top: 28rpx;
  color: #666;
  width: 400rpx;
}
.couponTime{
  font-size: 24rpx;
  color: #333;
  margin-left: 24rpx;
  margin-top:10rpx;
}
.getOut{
  width: 140rpx;
  line-height: 42rpx;
  height: 42rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  right: 48rpx;
  bottom: 80rpx;
  z-index: 22;
}
.getImg{
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}