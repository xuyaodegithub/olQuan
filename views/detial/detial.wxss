/* detial/detial.wxss */
page{
  width: 100%;
  background-color: #ffffff;  
  /* overflow: hidden; */
}
.content{
  background-color: rgb(242, 242, 242);
}
.content .price{
  position: relative;
  padding: 0 30rpx;
  margin-bottom: 2rpx;
  line-height: 80rpx;
  background-color: #ffffff;
}
.content .price .promise{
  font-size: 24rpx;
  color: rgb(119, 119, 119);
}
.content .price .bigTest{
  font-size: 38rpx;
  color: #333333;
  margin-right: 5rpx;
}
.content .price .delete{
  font-size: 28rpx;
  color: rgb(170, 170, 170);
}
.content .price image{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  right: 24rpx;
  top: 50%;
  margin-top: -20rpx;
}
.content .mess{
  padding: 0 30rpx;
  background-color: #ffffff;
  margin-bottom: 2rpx;
}
.content .title{
  font-size: 28rpx;
  padding: 20rpx 0;
  color: #333333;
}
.content .smalltitle{
  font-size: 24rpx;
  color: #777777;
}
.content .jindou{
  justify-content: space-between;
  font-size: 24rpx;
  color: #777777;
  line-height: 72rpx;
}
.content .tag{
  background-color: #ffffff;
  padding: 0 30rpx;
  margin-bottom: 20rpx;
  color: #333333;
  font-size: 24rpx;
  align-items: center;
  line-height: 80rpx
}
.content .tag > view{
  align-items: center;
}
.content .tag image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 12rpx;
  /* vertical-align: middle; */
}
.content .tag text{
  margin-right: 52rpx;
}
.content .scode{
  background-color: #ffffff;
  position: relative;
  padding: 0 30rpx;
  font-size: 26rpx;
  color: rgb(119, 119, 119);
  line-height: 90rpx;
  align-items: center;
  margin-bottom: 20rpx;
}
.content .scode text:nth-child(2){
  color: rgb(51, 51, 51);
  margin-left: 30rpx;
}
.content .scode image{
  width: 10rpx;
  height:20rpx;
  position:absolute;
  right: 24rpx;
  top: 50%;
  margin-top: -10rpx ;
  /* align-self: flex-end; */
}
.content .shareDou{
  background-color: #ffffff;
  padding: 0 30rpx;
  text-align: center;
  color: #333333;
  padding-bottom: 30rpx;
  margin-bottom: 20rpx;
}
.content .shareDou view:first-child{
  font-size: 32rpx;
  padding: 30rpx 0 34rpx 0;
}
.content .shareDou view:nth-child(2){
  font-size: 26rpx;
  padding-bottom: 40rpx;
}
.content .shareDou view:last-child{
    width: 400rpx;
    color: #E50F72;
    justify-content: space-between;
    text-align: center;
    margin: 0 auto;
}
.content .shareDou view:last-child button{
  width: 170rpx;
  border: 2rpx solid #E50F72;
  -webkit-border-radius:28rpx;
  -moz-border-radius:28rpx;
  -ms-border-radius: 28rpx;
  -o-border-radius: 28rpx;
  border-radius:28rpx;
  padding: 0;
  font-size: 28rpx;
  line-height: 54rpx;
  color: #E50F72;
}
.tryLiu,.withup{
  background-color: #ffffff;
  font-size: 26rpx;
  color: #333333;
  align-items: center;
}
.tryLiu{
  justify-content: space-between;
  line-height: 80rpx;
  margin-bottom: 2rpx;
  padding: 0 30rpx;  
}
.tryLiu > view{
  color: #777777;
}
.tryLiu image{
 margin-left: 12rpx; 
  width: 10rpx;
  height: 20rpx;
}
.withup{
  padding: 30rpx;
  justify-content: center;
  text-align: center;
  margin-bottom: 20rpx;
}
.withup image{
  display: block;
  width: 15rpx;
  height: 20rpx;
  margin:0 35rpx;
}
.withup .text{
  max-width:56rpx;
  height: 56rpx;
}
.tryBaogao,.instore{
background-color:rgb(242, 242, 242);
   /*  */
}
.tryImage{
padding: 20rpx 30rpx;
background-color: #ffffff;
margin-bottom: 2rpx;
}
.tryImage .user{
  justify-content: space-between;
  font-size: 24rpx;
  color: #8c8c8c;
  line-height: 60rpx;
  align-items: center;
  margin-bottom: 10rpx;
}

.tryImage .user image{
  display:block;
  width:60rpx;
  height:60rpx;
  margin-right: 24rpx;
}
.tryImage .tell{
  font-size: 26rpx;
  color: #333333;
  line-height: 46rpx;
  margin-bottom: 10rpx;
}
.tryImage .image {
  justify-content: space-between;
}
.tryImage .image image{
  display: block;
  width: 32%;
  height:240rpx;
}
.instore{
  padding: 10rpx 30rpx;
  justify-content: space-between;
  background-color: #ffffff;
  line-height: 80rpx;
  align-items: center;
  margin: 20rpx 0;  
}
.instore .store{
  font-size: 28rpx;
  line-height: 80rpx;
  /* color: #ffffff; */
  align-items: center;
}
.instore .store image{
  display: block;
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
}
.instore .house{
 width: 160rpx;
 justify-content: space-between;
 padding: 0 10rpx;
 align-items: center;
  border: 1px solid #999999;
  color: #333333;
  font-size: 24rpx;
  line-height: 56rpx;
  height: 56rpx;
  -webkit-border-radius:4rpx;
  -moz-border-radius:4rpx;
  -ms-border-radius: 4rpx;
  -o-border-radius: 4rpx;
  border-radius:4rpx;
}
.instore .house image{
  display: block;
  width: 34rpx;
  height:34rpx;
  /* margin-right: 10rpx; */
}
.h5Title{
  padding: 0 30rpx;
  background-color: #ffffff;
  font-size: 30rpx;
  color: rgb(51, 51, 51);
  line-height: 80rpx;
  text-align: center;
}
.content .tryPrice{
  line-height: 94rpx;
  position: relative;
  z-index: 2;
  padding-left: 30rpx ;
  justify-content: space-between;
}
.content .addposition{
  position: relative;
}
.content .addposition image{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 1;
  top: 0;
}
.tryPrice .left text:first-child{
  font-size: 24rpx;
  color: rgb(221, 221, 221);
}
.tryPrice .left text:last-child{
  font-size: 38rpx;
  color: #ffffff;
}
.tryPrice .right{
  color:#ffffff;
  font-size: 24rpx;
  
}