/* views/detial/toSureBuy/toSureBuy.wxss */
page{
  background-color: #f7f7f7;
  padding-bottom: 120rpx;
}
.moadress{
  margin-top: 20rpx;
  padding:48rpx 20rpx 58rpx 30rpx;
  background-color: #ffffff;
  font-size: 28rpx;
  color: #666666;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.moadress .top{
  margin-bottom: 36rpx;
}
 .moadress .top text:first-child{
  color: rgb(51, 51, 51);
  margin-right: 30rpx;
}
.moadress .adress text{
  margin-right: 15rpx
}
.moadress image{
  display: block;
  width: 16rpx;
  height: 26rpx;
}
.seller{
  background-color: #ffffff;
  padding: 0 30rpx;
  font-size: 28rpx;
  color: #333333;
  line-height: 84rpx;
}
.pMess .flex{
  /* background-color: #f7f7f7; */
  padding:20rpx 40rpx 20rpx 30rpx;
}
.pMess .flex > image{
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin-right: 30rpx;
}
.pMess .flex .center{
  width: 40%;
  font-size: 26rpx;
  line-height: 48rpx;
  color: #333333;
}
.pMess .flex .center .color{
  font-size: 24rpx;
  color:#666666;
  /* width:50%; */
  line-height: 36rpx;
  margin-top: 10rpx;
}
.pMess > .flex .right{
  flex: 1;
}
.pMess >.flex .right{
  font-size: 24rpx;
  color: #666;
  text-align: right;
}
.pMess .right text{
  display: block;
}
.pMess .right text:first-child{
  color: #333333;
  line-height: 48rpx;
  margin-bottom:34rpx; 
  }
  .pMess .right text:last-child{
    margin-top: 10rpx;
  }
  .pMess .fee,.pMess .file,.userMess > view:first-child,.userMess .anyPay{
    justify-content: space-between;
    background-color: #fff;
    font-size: 28rpx;
    line-height: 84rpx;
    color: #333;
    padding: 0 30rpx;
    border-bottom: 2rpx solid #e5e5e5;
  }
  .pMess .file{
    justify-content: flex-start;
    margin-bottom: 20rpx;
  }
  .pMess .file input{
    height: 84rpx;
    flex: 1;
  }
  .pMess .fee text:last-child{
    color: #707070;
  }
  .userMess .anyPay {
    justify-content: space-between;
    align-items: center;
  }
  .userMess .anyPay .right .border,.userMess .anyPay .right image,.couponList .border,.couponList .right .flex image,.paymemoy .flex .border{
      width: 40rpx;
      height: 40rpx;
      border-radius: 50%;
      border: 2rpx solid #C7D3DE;
  }
  .userMess .anyPay .right image,.couponList .right .flex image{
    border-radius: none;
    margin-right: 0;
  }
  .userMess .anyPay text{
    font-size: 24rpx;
    color: #888888;
  }
  .userMess .anyPay .right{
   justify-content: flex-end;
    padding: 0;    
    align-items: center;
  }
  .userMess .anyPay .right .elenum text{
    color: #d33778;
    margin:0 20rpx 0 0;
  }
 .userMess .checkedBottom{
   background-color: #fff;
   padding: 0 34rpx;
   font-size: 28rpx;
   line-height: 84rpx;
   color: #333;
   align-items: center;
  }
  .userMess .checkedBottom text{
    font-size: 24rpx;
    color: rgb(136, 136, 136);
  }
  .userMess .checkedBottom input{
      display: inline-block;
      height: 48rpx;
      width: 178rpx;
      margin-right: 20rpx;
      vertical-align: middle;
      border: 2rpx solid #b5b5b5;
      padding-left:15rpx; 
  }
  .userMess view.couponClass{
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    line-height: 84rpx;
    padding: 0 30rpx;
    background-color: #fff;
    margin-bottom: 20rpx;
  }
  .couponClass .left text{
    font-size: 24rpx;
    color: #888888
  }
  .couponClass .right{
    justify-content: space-between;
    padding: 0;
    text-align: right;
    align-items: center;
  }
  .couponClass .right image{
    display: block;
    width: 16rpx;
    height: 26rpx;
    margin: 0;
    margin-left: 30rpx;
  }
 .couponList .onecoupon{
  position: relative;
}
.couponList .onecoupon > image{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 1;
  top: 0;
}
.couponList{
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    z-index: 99;
    background: rgba(0,0,0,.5);
 }
 .couponContent{
   position: absolute;
   left: 0;
   width: 100%;
   bottom: 0;
   background-color: #ffffff;
   z-index: 111
 }
 .couponList .top{
  padding: 0 24rpx;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx solid rgb(242, 242, 242);
  font-size: 25rpx;
  line-height: 88rpx;
}
.couponList .list{
padding: 0 24rpx;
font-size: 24rpx;
max-height: 680rpx;
overflow-y: auto;
}
.couponList .flex .left{
line-height: 160rpx;
text-align: center;
color: #ed0276;
font-size: 60rpx;
width:200rpx;
}
.couponList .flex .right{
  flex: 1;
  font-size: 24rpx;
  color: #000000;
  padding: 0 20rpx;
}
.couponList .right > view:first-child{
  line-height: 80rpx;
  margin-bottom: 28rpx;
}
.couponList .right .flex{
  justify-content: space-between;
  line-height: 48rpx;
  color: #666666;
  align-content: center;
  margin-bottom: 14rpx;
}
.couponList .right .btn{
  width: 140rpx;
  /* line-height: normal; */
  color: #fff;
  text-align: center;
  background-color: #ed0276;
  border-radius:48rpx;
}
.couponList .right .canotget{
  background-color: #BDBCBC;
}
.couponList .bottom{
  color: #333;
  font-size: 24rpx;
  line-height: 64rpx;
  padding: 0 24rpx;
}
.couponList .onecoupon{
  margin-bottom: 20rpx;
}
.couponContent .top .close{
   display: block;
   width: 32rpx;
   height: 32rpx;
   margin: 0;
 }
 .couponList .right .flex{
   justify-content: flex-end;
 }
  .couponList .right .flex image,.couponList .right .flex .border{
   position: absolute;
   z-index: 2000;
   top: 50%;
   margin-top: -20rpx;
   right: 48rpx;
  }
  .allPay{
    background-color: #fff;
    padding: 0 30rpx;
    color:#919191;
    margin-bottom: 20rpx;
  }
  .allPay > text:first-child{
    font-size: 28rpx;
    line-height: 72rpx;
  }
  .allPay .flex{
    justify-content: space-between;
    font-size: 24rpx;
    line-height: 46rpx;
    padding: 0;
  }
  .allPay .flex text:last-child{
    color: #d33778;
  }
  .paymemoy{
    background-color: #fff;
    font-size: 24rpx;
    line-height: 56rpx;
    color: #333;
  }
  .paymemoy > view:first-child{
    border-bottom: 2rpx solid #e5e5e5;
    padding: 0 30rpx;    
  }
  .paymemoy > .flex{
    justify-content: space-between;
    border-bottom: 2rpx solid #e5e5e5;
    padding: 24rpx 30rpx;
  }
  .paymemoy .flex .left image{
      display: block;
      width: 60rpx;
      height: 60rpx;
      margin-right: 10rpx;
  }
  .paymemoy .flex .flex{
    padding: 0;
  }
  .paymemoy .right image{
      display: inline-block;
      width: 40rpx;
      height: 40rpx;
      vertical-align: middle;
  }
  .surebuy{
    background-color: #fff;
    padding: 0 30rpx;
    justify-content: space-between;
    font-size: 30rpx;
    line-height: 120rpx;
    position: fixed;
    left: 0;
    bottom: 0;
   width:-webkit-calc(100% - 60rpx); 
   width:-moz-calc(100% - 60rpx); 
   width:calc(100% - 60rpx);
   align-items: center;
   color: #888;
   border-top: 2rpx solid #e1e1e1;
   z-index:555;
  }
  .surebuy view:first-child text:first-child{
      color: rgb(230, 29, 121);
      margin-right: 10rpx
  }
  .surebuy view:last-child {
    width: 30%;
    background-color: #e61d79;
    line-height: 80rpx;
    border-radius: 8rpx;
    text-align: center;
    color: #ffffff;
    font-size: 32rpx;
  }
  .password{
    position: fixed;
    background-color: #ffffff;
    height: 470rpx;
    width: 85%;
    top: 50%;
    left: 50%;
    margin: -235rpx 0 0 -42.5%;
    z-index: 999;
  }
  .password .title{
    text-align: center;
    font-size: 30rpx;
    padding-top: 60rpx;
  }
  .password input{
    margin: 80rpx auto;
    width: 80%;
    border: 2rpx solid #e5e5e5;
    padding-left:20rpx;
    line-height:100rpx;
    font-size: 32rpx;
    height:100rpx;
  }
  .password .flex{
    font-size: 30rpx;
    text-align: center;
    line-height: 100rpx;
    color: #000;
    border-top: 2rpx solid #e5e5e5;
  }
  .password .flex view{
    flex: 1
  }
  .password .flex view:last-child{
    border-left: 1px solid rgb(229, 229, 229);
    color:rgb(231, 49, 125);
  }