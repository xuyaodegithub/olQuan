<!--detial/detial.wxml-->
<swiper indicator-dots="{{indicatorDots}}"
  autoplay interval="3000" duration="400" style="height:340rpx">
  <block wx:for="{{imgUrls}}" wx:key="{{index}}">
    <swiper-item>
      <image src="{{item}}" mode="widthFix" style="width:100%;" />
    </swiper-item>
  </block>
</swiper>
